Table Users {
    id_Users serial [pk]
    username varchar [unique]
    email varchar
    contrasena varchar(10)
}
Table planes {
  id_planes   int
  nombre      varchar
  precio_usd  decimal
}
Table suscripciones {
  id_suscripciones  int
  usuario_id  int
  plan_id     int
  fecha_inicio date
  fecha_fin   date
}
Table pagos {
  id_pagos          int
  suscripcion_id int
  monto_usd   decimal
  estado      varchar
  pagado_en   timestamp
}
Table estado_p {
    id_estado_p  int
    completado varchar
    fallido varchar
}
Table perfiles {
  id_perfiles         int
  usuario_id  int
  nombre      varchar
  edad        int
  idioma      varchar
  historial   varchar

}
Table titulos {
  id_titulos          int
  nombre      varchar
  tipo        varchar   // pel√≠cula o serie
  generos varchar
  sinopsis    text
  ano_estreno int
}
Table tipos {
    id_tipos  int
    peliculas varchar
    series varchar
}
Table peliculas {
    id_peliculas int 
    nombre varchar
    generos varchar
    duracion time
}
Table serie {
  id_serie        int
  titulo_id   int
  temporada   int
  numero      int
  nombre      varchar
  generos varchar
}
Table generos {
  id_generos int
  terror   varchar
  accion  varchar
  romance varchar
  anime varchar
  risa varchar
  documentales varchar
}
Table historial {
  id_historial          int
  titulo_id   int
  episodio_id int
  visto_en   varchar
}

Ref: Users.id_Users < planes.id_planes
Ref: planes.id_planes < suscripciones.id_suscripciones
Ref: suscripciones.id_suscripciones < pagos.id_pagos
Ref: pagos.estado < estado_p.id_estado_p
Ref: estado_p.fallido < pagos.id_pagos
ref: estado_p.completado < perfiles.id_perfiles
Ref: perfiles.id_perfiles < titulos.id_titulos
ref: titulos.tipo < tipos.id_tipos
Ref: tipos.peliculas < peliculas.id_peliculas
Ref: tipos.series < serie.id_serie
Ref: titulos.generos < generos.id_generos
Ref: peliculas.generos < generos.id_generos
Ref: serie.generos < generos.id_generos
Ref: generos.id_generos < historial.id_historial
Ref: perfiles.historial < historial.id_historial
